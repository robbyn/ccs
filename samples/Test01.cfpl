VAR a=100, b=200, c=300 AS INT
VAR f=c+1 AS FLOAT
VAR d="TRUE" AS BOOL 
START
    OUTPUT: f
    OUTPUT: "Hello world!!!"
    OUTPUT: d & " " & -c
    OUTPUT: (a<b) & " " & (a>b) & " " & (a<=b) & " " & (a>=b) & " " & (a==b) & " " & (a<>b)
    IF (d)
        START
            OUTPUT: "TRUE"
        STOP

    ELSE
        START
            OUTPUT: "FALSE"
        STOP
    c=1
    WHILE (c<=5)
        START
            IF (c%2 <> 0)
               START
                  OUTPUT: "c=" & c
               STOP
            c=c+1
        STOP
    OUTPUT: sin(b)
STOP 
